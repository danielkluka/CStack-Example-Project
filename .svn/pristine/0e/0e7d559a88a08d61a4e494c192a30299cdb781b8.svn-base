/*	Private method Compare
 *	author Pesek
 */
int CStack::Compare(const CStack& aStack) const
	{
	if (this->Actual() > aStack.Actual())
		{
		return 1;
		}
	else if (this->Actual() < aStack.Actual())
		{
		return -1;
		}
	else if (this->Actual() == aStack.Actual())
		{
		return 0;
		}
	}

