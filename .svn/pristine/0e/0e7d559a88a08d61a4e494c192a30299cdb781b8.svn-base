/*	Stack Search
 *	authors Prasek, Pesek
 */
bool CStack::Search(const CBlock &aValue) const
	{
	const CBlock *iCurrent = iTop;

	while(iCurrent != NULL)
		{
		if(iCurrent == aValue.NextItem())
			return true;

		iCurrent = static_cast<CBlock *>(iTop->NextItem());
		}

	return false;
	}
	
